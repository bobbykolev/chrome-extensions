{
  "manifest_version": 2,

  "name": "YouTube Extend Plugin",
  "description": "Loop and download youtube videos. Using listenonrepeat.com and youtube-mp3.org",
  "version": "1.0",

  "browser_action": {
   "default_icon": "iconRound.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
    "http://*/*"
   ],
  "icons": { 
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}